{
  "name": "chain-able",
  "version": "1.0.1",
  "description": "next level chaining.",
  "main": "./index.cjs.js",
  "module": "./index.es.js",
  "web": "./index.js",
  "browser": "./index.js",
  "alias": "./index.js",
  "scripts": {
    "strip": "flow-remove-types src/ --pretty --all --out-dir ./",
    "strip-dist": "flow-remove-types src/ --pretty --all --out-dir ./dist",
    "prepublish": "npm run strip",
    "pretest": "npm run strip",
    "test": "ava --verbose",
    "build": "npm run strip && npm run strip-dist && rollup -c"
  },
  "devDependencies": {
    "ava": "*",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "flow-remove-types": "*",
    "fuse-box": "^2.0.0",
    "inspector-gadget": "^1.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "source": [
      "src/**",
      "!dist/**/*"
    ],
    "verbose": true,
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "keywords": [
    "lego",
    "chain-able",
    "conditional",
    "fluent",
    "chain",
    "flipchain",
    "fliphub-chain",
    "fusebox-chain",
    "webpack",
    "fusebox"
  ],
  "license": "MIT",
  "author": "James <aretecode@gmail.com>",
  "homepage": "https://github.com/fluents/chain-able#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fluents/chain-able.git"
  },
  "bugs": {
    "url": "https://github.com/fluents/chain-able/issues"
  },
  "directories": {
    "lib": "src",
    "test": "test",
    "deps": "deps"
  },
  "files": [
    "index.js",
    "src",
    "deps",
    "dist",
    "index.cjs.js",
    "index.es.js",
    "Chainable.all.js",
    "Chainable.js",
    "Chainable.node.js",
    "ChainedMap.js",
    "ChainedMapExtendable.js",
    "ChainedSet.js",
    "ChildChain.js",
    "FactoryChain.js",
    "ImmutableChain.js",
    "MergeChain.js",
    "TypeChain.js"
  ]
}
